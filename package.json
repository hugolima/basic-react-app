{
  "name": "basic-react-app",
  "version": "1.0.0",
  "description": "Basic React Application",
  "main": "./server/app.js",
  "homepage": "http://basic-react-app.herokuapp.com",
  "engines": {
    "node": "6.0.0"
  },
  "scripts": {
    "start-dev": "node ./server/app.js | webpack -d --watch",
    "build-dev": "NODE_ENV=production webpack -p",
    "postinstall": "webpack -p",
    "start": "node ./server/app.js",
    "test": "jest"
  },
  "keywords": [
    "npm",
    "webpack",
    "react",
    "redux",
    "bootstrap"
  ],
  "author": "Hugo Lima",
  "repository": {
    "type": "git",
    "url": "git@github.com:hugolima/basic-react-app.git"
  },
  "license": "MIT",
  "jest": {
    "verbose": "true",
    "moduleNameMapper": {
      "^server/validations.js": "<rootDir>/server/validations.js",
      "^server/config.js": "<rootDir>/server/config.js",
      "^.+\\.(css|scss)$": "<rootDir>/client/src/__mocks__/styleMock.js"
    }
  },
  "dependencies": {
    "babel-core": "^6.18.0",
    "babel-loader": "^6.2.7",
    "babel-plugin-transform-object-assign": "^6.8.0",
    "babel-plugin-transform-object-rest-spread": "^6.16.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "body-parser": "^1.15.2",
    "bootstrap-sass": "^3.3.7",
    "compression": "^1.6.2",
    "css-loader": "^0.25.0",
    "eslint": "^3.8.1",
    "eslint-config-airbnb": "^12.0.0",
    "eslint-loader": "^1.6.0",
    "eslint-plugin-import": "^2.0.1",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.4.1",
    "expose-loader": "^0.7.1",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.24.0",
    "isomorphic-fetch": "^2.2.1",
    "jquery": "^3.1.1",
    "node-sass": "^3.10.1",
    "postcss-loader": "^1.0.0",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-redux": "^4.4.5",
    "redux": "^3.6.0",
    "redux-thunk": "^2.1.0",
    "sass-loader": "^4.0.2",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.3"
  },
  "devDependencies": {
    "babel-jest": "^16.0.0",
    "deep-freeze": "0.0.1",
    "enzyme": "^2.5.1",
    "jest": "^16.0.2",
    "nock": "^9.0.0",
    "react-addons-test-utils": "^15.3.2",
    "redux-mock-store": "^1.2.1"
  }
}
